generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL") // <--- This is REQUIRED in version 6
}

model Analysis {
  id         String   @id @default(uuid())
  userId     String
  fileName   String
  topClass   String
  confidence Float
  createdAt  DateTime @default(now())

  @@index([userId])
}